# Environment configuration for Nakama X4 Client
# Copy this file to .env and adjust paths for your local setup

# EmmyLua debugger path (adjust for your VS Code installation)
# Find this path in VS Code: Help > About > Extension folder location
# Then navigate to: tangzx.emmylua-[version]/debugger/emmy/windows/x64/?.dll
EMMYLUA_DEBUGGER_PATH=c:/Users/YOUR_USERNAME/.vscode/extensions/tangzx.emmylua-0.9.30-win32-x64/debugger/emmy/windows/x64/?.dll

# Debug settings
DEBUG_PORT=9966
DEBUG_HOST=localhost